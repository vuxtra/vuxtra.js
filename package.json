{
  "name": "vuxtra",
  "version": "0.1.0",
  "description": "Vuxtra - Realtime based full stack framework based on (VUe + nuXT + extRA) ",
  "main": "index.js",
  "bin": {
    "vuxtra": "./bin/vuxtra"
  },
  "scripts": {
    "test": "npm run lint && cross-env NODE_ENV=test npm run build:vuxtra && nyc ava --verbose --serial test/ -- && nyc report --reporter=html",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "lint": "eslint --ext .js,.vue bin lib pages test/*.js --ignore-pattern app",
    "build": "rimraf dist/ && npm run build:all",
    "build:all": "rollup -c build/rollup.config.js",
    "watch": "npm run build:vuxtra -- -w"
  },
  "engines": {
    "node": ">= 8.7.0",
    "npm": ">= 5.3.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vuxtra/vuxtra.js.git"
  },
  "keywords": [
    "vue.js",
    "nuxt.js",
    "koa",
    "api",
    "realtime",
    "javascript"
  ],
  "author": "Faruk Brbovic @fbrbovic",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vuxtra/vuxtra.js/issues"
  },
  "homepage": "https://github.com/vuxtra/vuxtra.js#readme",
  "dependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-vue-app": "^1.3.1",
    "chokidar": "^1.7.0",
    "connect": "3.0.1",
    "cross-spawn": "^5.1.0",
    "debug": "^3.1.0",
    "express": "^4.16.2",
    "fs-extra": "^4.0.2",
    "glob": "^7.1.2",
    "lodash": "^4.17.4",
    "memory-fs": "^0.4.1",
    "minimist": "^1.2.0",
    "morgan": "1.7.0",
    "nuxt": "^1.0.0-rc11",
    "sc-errors": "~1.3.3",
    "sc-framework-health-check": "~1.0.0",
    "sc-hot-reboot": "~1.0.0",
    "scc-broker-client": "~1.5.0",
    "serve-static": "1.11.2",
    "socketcluster": "~8.0.2",
    "socketcluster-client": "^8.0.1",
    "tapable": "^1.0.0-beta.1",
    "un-eval": "^1.1.0",
    "webpack": "^3.7.1",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.19.1",
    "webpack-node-externals": "^1.6.0"
  },
  "devDependencies": {
    "ava": "^0.22.0",
    "babel-eslint": "^8.0.1",
    "babel-plugin-array-includes": "^2.0.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-istanbul": "^4.1.5",
    "codecov": "^2.3.0",
    "cross-env": "^5.0.5",
    "eslint": "^4.8.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-html": "^3.2.2",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.2.0",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "nyc": "^11.2.1",
    "rimraf": "^2.6.2",
    "rollup": "^0.50.0",
    "rollup-plugin-alias": "^1.3.1",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-commonjs": "^8.2.1",
    "rollup-plugin-multi-dest": "^1.0.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-watch": "^4.3.1"
  }
}
